<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include user preferences and favorites -->
        <include domain="shared_pref" path="favorites.xml" />
        <include domain="shared_pref" path="settings.xml" />
        <include domain="shared_pref" path="watch_history.xml" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Include user preferences and favorites -->
        <include domain="shared_pref" path="favorites.xml" />
        <include domain="shared_pref" path="settings.xml" />
        <include domain="shared_pref" path="watch_history.xml" />
    </device-transfer>
</data-extraction-rules> 